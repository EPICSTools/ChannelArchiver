<?xml version="1.0" encoding="UTF-8"?>
<!-- DTD for the archiveconfig -->
<!ELEMENT archiveconfig (daemon*)>

<!ELEMENT daemon (disable-check?,desc?,port,engine*)>
<!ATTLIST daemon directory CDATA #REQUIRED>

<!ELEMENT engine (disable-check?,desc?,port,restart?)>
<!ATTLIST engine directory CDATA #REQUIRED>

<!ELEMENT disable-check (#PCDATA)> <!-- true|false -->
<!ELEMENT desc (#PCDATA)>
<!ELEMENT port (#PCDATA)>

<!ELEMENT restart (#PCDATA)>
<!ATTLIST restart type CDATA #REQUIRED>
